// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Curso {
  id    String @id @default(uuid())
  nome  String
  descricao String
  cargaHoraria Int
}

model Servico {
  id    String @id @default(uuid())
  nome String
  descricao String
}

model Equipamento {
  id    String @id @default(uuid())
  nome String
  descricao String
}
